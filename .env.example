# TheNewsAPI.com API key
THENEWS_API_KEY=your_thenews_api_key_here

# Huggingface API token for model access
HF_API_TOKEN=your_huggingface_api_token_here

# Discord webhook URL สำหรับส่งข้อความ
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_id

# Pinecone API key และ environment (ถ้าใช้ Pinecone)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENV=us-west1-gcp

# Vector DB host และพอร์ต (ถ้าใช้ Milvus หรืออื่น ๆ แบบ self-host)
VECTOR_DB_HOST=localhost
VECTOR_DB_PORT=19530
